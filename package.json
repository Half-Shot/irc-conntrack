{
  "name": "irc-conntrack",
  "version": "0.0.1",
  "description": "A suitably dumb irc connection tracker.",
  "main": "index.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "tsc",
    "lint": "tslint --project tsconfig.json --format stylish",
    "start": "node build/src/Program.js",
    "ircctl": "node build/tools/ircctl/ircctl.js",
    "test": "mocha -r ts-node/register test/Configure.ts test/**/Test*.ts",
    "coverage": "nyc mocha --require ts-node/register --require source-map-support/register --full-trace --bail test/**/*.ts test/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Half-Shot/irc-conntrack.git"
  },
  "keywords": [],
  "author": "Will Hunt <will@half-shot.uk>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Half-Shot/irc-conntrack/issues"
  },
  "homepage": "https://github.com/Half-Shot/irc-conntrack",
  "dependencies": {
    "@types/inquirer": "^6.5.0",
    "commander": "^4.1.1",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "http-status-codes": "^1.4.0",
    "inquirer": "^7.0.4",
    "inquirer-command-prompt": "^0.0.23",
    "irc-colors": "^1.5.0",
    "js-yaml": "^3.13.1",
    "moment": "^2.29.2",
    "prom-client": "^11.5.3",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "uuid": "^3.4.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/chai-as-promised": "^7.1.0",
    "@types/express": "^4.17.2",
    "@types/express-ws": "^3.0.0",
    "@types/js-yaml": "^3.11.2",
    "@types/mocha": "^5.2.5",
    "@types/mock-require": "^2.0.0",
    "@types/request-promise-native": "^1.0.17",
    "@types/uuid": "^3.4.3",
    "@types/ws": "^7.2.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^5.2.0",
    "mock-require": "^3.0.2",
    "nyc": "^15.0.0",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.7.5"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": false,
    "lines": 85,
    "statements": 85,
    "functions": 75,
    "branches": 75,
    "include": [
      "src"
    ],
    "exclude": [
      "src/Program.ts"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "cache": true,
    "all": true,
    "instrument": true,
    "sourceMap": true,
    "report-dir": "./.nyc"
  }
}
