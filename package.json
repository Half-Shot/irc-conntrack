{
  "name": "irc-conntrack",
  "version": "0.0.1",
  "description": "A suitably dumb irc connection tracker.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node build/src/Program.js",
    "test": "npm run build && mocha build/test",
    "coverage": "npm run build && nyc mocha build/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Half-Shot/irc-conntrack.git"
  },
  "keywords": [],
  "author": "Will Hunt <will@half-shot.uk>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Half-Shot/irc-conntrack/issues"
  },
  "homepage": "https://github.com/Half-Shot/irc-conntrack",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "chai": "^4.1.2",
    "mock-require": "^3.0.2"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "@types/express-ws": "^3.0.0",
    "@types/js-yaml": "^3.11.2",
    "@types/mock-require": "^2.0.0",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.4.4",
    "@types/ws": "^6.0.0",
    "commander": "^2.17.1",
    "express": "^4.16.3",
    "express-ws": "^4.0.0",
    "irc-colors": "^1.4.3",
    "js-yaml": "^3.12.0",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "nyc": "^12.0.2",
    "typescript": "^3.0.1",
    "uuid": "^3.3.2",
    "winston": "^3.0.0",
    "winston-daily-rotate-file": "^3.3.2",
    "ws": "^6.0.0"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "sourceMap": true,
    "include": [
      "build/**/*.js"
    ],
    "exclude": [
      "build/test/**/*"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "extension": [
      ".js"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./.nyc"
  }
}
